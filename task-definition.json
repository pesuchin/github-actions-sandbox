{
    "family": "analysis-db-api",
    "executionRoleArn": "arn:aws:iam::464057627735:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "disableNetworking": false,
    "containerDefinitions": [
        {
            "name": "analysis-db-api-development",
            "image": "464057627735.dkr.ecr.us-east-2.amazonaws.com/analysis-db-api/analysis-db-api:latest",
            "cpu": 0,
            "memory": 128,
            "essential": true,
            "privileged": false,
            "readonlyRootFilesystem": false,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8000/ || exit 1"
                ],
                "interval": 30,
                "timeout": 20,
                "retries": 3
            },
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}